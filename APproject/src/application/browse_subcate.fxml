<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.browse3_controller">
   <top>
      <HBox prefHeight="100.0" prefWidth="300.0" style="-fx-background-color: teal;" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="Back" mnemonicParsing="false" onAction="#goback" text="Back" />
            <Label text="Browse subcategories" textFill="#fcfcfc">
               <font>
                  <Font size="24.0" />
               </font>
               <HBox.margin>
                  <Insets left="-50.0" top="70.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </top>
   <bottom>
      <HBox prefHeight="78.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <children>
            <Label prefHeight="17.0" prefWidth="271.0" text="Enter selected subcategory">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#go_search" text="Select">
               <HBox.margin>
                  <Insets right="90.0" top="50.0" />
               </HBox.margin>
            </Button>
            <TextField>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
   </bottom>
   <center>
      <HBox prefHeight="235.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <children>
            <TextArea maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="264.0" prefWidth="400.0" />
         </children>
      </HBox>
   </center>
</BorderPane>
