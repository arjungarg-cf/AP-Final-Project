<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.end_user_actual_entry_controller">
   <top>
      <HBox prefHeight="115.0" prefWidth="413.0" style="-fx-background-color: teal;" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="Back" cancelButton="true" mnemonicParsing="false" onAction="#goback" text="Back" />
            <Label text="End User" textFill="#fcfcfc">
               <font>
                  <Font size="24.0" />
               </font>
               <HBox.margin>
                  <Insets top="80.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </top>
   <left>
      <HBox prefHeight="217.0" prefWidth="413.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#gobrowse" text="Browse">
               <HBox.margin>
                  <Insets left="100.0" top="50.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#go_search_from_end_user" text="Check availability">
               <HBox.margin>
                  <Insets left="60.0" top="50.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </left>
</BorderPane>
