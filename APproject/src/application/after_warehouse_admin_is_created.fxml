<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="application.after_warehouse_admin_is_created_controller">
   <top>
      <HBox prefHeight="50.0" prefWidth="400.0" style="-fx-background-color: teal;" BorderPane.alignment="CENTER" />
   </top>
   <left>
      <HBox prefHeight="100.0" prefWidth="400.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Warehouse admin successfully created!">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </left>
   <bottom>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#gocontinuetocreate" text="Continue creating!">
               <HBox.margin>
                  <Insets left="60.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#gohome" text="Home">
               <HBox.margin>
                  <Insets left="40.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
