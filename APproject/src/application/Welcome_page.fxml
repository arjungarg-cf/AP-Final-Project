<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Main_controller">
   <center>
      <BorderPane prefHeight="600.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <top>
            <HBox prefHeight="183.0" prefWidth="600.0" style="-fx-background-color: teal;" BorderPane.alignment="CENTER">
               <children>
                  <TextField prefHeight="68.0" prefWidth="233.0" text="WELCOME">
                     <HBox.margin>
                        <Insets bottom="70.0" left="170.0" right="170.0" top="70.0" />
                     </HBox.margin>
                     <font>
                        <Font name="System Bold" size="36.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
         </top>
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <TextField prefHeight="74.0" prefWidth="600.0" text="Proceed as:">
                     <font>
                        <Font size="18.0" />
                     </font></TextField>
                  <VBox prefHeight="230.0" prefWidth="600.0">
                     <children>
                        <TilePane prefHeight="207.0" prefWidth="600.0">
                           <children>
                              <Button mnemonicParsing="false" text="Store Administrator">
                                 <TilePane.margin>
                                    <Insets top="30.0" />
                                 </TilePane.margin>
                              </Button>
                              <Button mnemonicParsing="false" onAction="#gosuper" text="Super User">
                                 <TilePane.margin>
                                    <Insets top="30.0" />
                                 </TilePane.margin>
                              </Button>
                              <Button mnemonicParsing="false" text="End User">
                                 <TilePane.margin>
                                    <Insets top="60.0" />
                                 </TilePane.margin>
                              </Button>
                              <Button mnemonicParsing="false" text="Warehouse administrator">
                                 <TilePane.margin>
                                    <Insets left="20.0" top="30.0" />
                                 </TilePane.margin>
                              </Button>
                           </children>
                        </TilePane>
                     </children>
                  </VBox>
               </children>
            </VBox>
         </center>
      </BorderPane>
   </center>
</BorderPane>
