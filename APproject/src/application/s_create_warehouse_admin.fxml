<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1">
   <center>
      <BorderPane prefHeight="600.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <top>
            <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: teal;" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" text="&lt;--Back" />
                  <TextField prefHeight="27.0" prefWidth="215.0" text="Create warehouse administrator">
                     <HBox.margin>
                        <Insets left="140.0" top="30.0" />
                     </HBox.margin>
                  </TextField>
               </children>
            </HBox>
         </top>
         <bottom>
            <HBox prefHeight="54.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" text="Create">
                     <HBox.margin>
                        <Insets left="270.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </bottom>
         <center>
            <TableView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn prefWidth="75.0" text="Login ID" />
                <TableColumn prefWidth="75.0" text="Password" />
              </columns>
            </TableView>
         </center>
      </BorderPane>
   </center>
   <bottom>
      <HBox prefHeight="51.0" prefWidth="600.0" style="-fx-background-color: teal;" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" text="HOME">
               <HBox.margin>
                  <Insets left="270.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
</BorderPane>
